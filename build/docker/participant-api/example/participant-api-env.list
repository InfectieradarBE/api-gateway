# listen ports:
GATEWAY_LISTEN_PORT=<port>
DEBUG_MODE=false
CORS_ALLOW_ORIGINS=https://your-domain.tld,https://test-domain.tld

# clients:
ADDR_USER_MANAGEMENT_SERVICE=<user-m-addr>:<user-m-port>
ADDR_STUDY_SERVICE=<study-addr>:<study-port>

USE_DELETE_PARTICIPANT_DATA_ENDPOINT=true
DISABLE_SIGNUP_WITH_EMAIL_ENDPOINT=false

USE_RECAPTCHA=true
RECAPTCHA_SECRET=<secret key>